{
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^3.1",
    "phpunit/phpunit": "^9.5",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "WCKoban\\": "src/includes",
      "WCKoban\\Admin\\": "src/admin",
      "WCKoban\\Hooks\\": "src/includes/hooks",
      "WCKoban\\Serializers\\": "src/includes/serializers"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WCKoban\\Tests\\": "tests",
      "WCKoban\\Tests\\Mocks\\": "tests/mocks"
    },
    "classmap": [
      "tests/mocks/MockResponse.php"
    ]
  }
}
